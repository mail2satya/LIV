{
  "name": "liv",
  "version": "1.0.0",
  "description": "A store application named LIV",
  "repository": "https://github.com/your-github-username/LIV.git",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js-community/odata-v2-adapter": "^1",
    "@cap-js/sqlite": "^1.1.0",
    "@sap/cds": "^7",
    "@sap/cds-dk": "^7",
    "@sap/cds-odata-v2-adapter-proxy": "^1.9.21",
    "@sap/hana-client": "^2.25.29",
    "@sap/hdi-deploy": "^4",
    "express": "^4.18.3",
    "hdb": "^2.25.1"
  },
  "devDependencies": {
    "@sap/ux-specification": "^1.120.1",
    "rimraf": "^3.0.2",
    "sqlite3": "^5.1.6"
  },
  "scripts": {
    "start": "cds run",
    "build": "cds build",
    "deploy": "cf create-service-push"
  },
  "cds": {
    "build": {
      "target": "gen"
    },
    "requires": {
      "db": {
        "kind": "hana",
        "model": [
          "db",
          "srv"
        ],
        "migrations": "db/migrations"
      }
    },
    "migrations": {
      "table": {
        "name": "cds_migrations"
      }
    },
    "credentials": {
      "database": "db.sqlite"
    }
  },
  "odata": {
    "version": "v2",
    "service": "LivService"
  }
}
